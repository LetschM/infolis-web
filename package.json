{
  "name": "infolis-web",
  "version": "0.0.1",
  "description": "Infolis Web services",
  "scripts": {
    "test": "coffee test/*.coffee | tap-spec",
    "docs": "docker -i src -o apidocs -s -n -c vs",
    "compile": "coffee -c -o lib src",
    "clean": "rm -r apidocs lib ; true",
    "serve": "nodemon src/infolis-web.coffee -w src -w ../express-jsonld/src -w ../mongoose-jsonld-express/src -w ../infolis-schema/src -w ../mongoose-jsonld/src",
    "mongo": "mongod --dbpath data/mongodb --port 27018",
    "run": "npm run mongo & npm run serve",
    "prepublish": "npm run compile"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/infolis/infolis-web"
  },
  "author": "kba",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/infolis/infolis-web/issues"
  },
  "homepage": "https://github.com/infolis/infolis-web",
  "dependencies": {
    "accepts": "^1.2.5",
    "async": "^0.9.0",
    "body-parser": "^1.12.2",
    "chalk": "^1.0.0",
    "express": "^4.12.2",
    "infolis-schema": "0.0.3",
    "express-jsonld": "0.0.9",
    "merge": "^1.2.0",
    "mongoose": "^4.0.1",
    "mongoose-jsonld": "^0.1.13",
    "multiparty": "^4.1.1",
    "superagent": "^1.1.0",
    "tape": "^4.0.0"
  },
  "devDependencies": {
    "coffee-script": "^1.9.1",
    "docker": "^0.2.14",
    "tap-spec": "^2.2.2",
    "tapes": "^0.4.1"
  },
  "main": "lib/index.js"
}
