{
  "name": "infolis-web",
  "version": "0.0.4",
  "description": "Infolis Web services",
  "scripts": {
    "test": "coffee test/*.coffee | tap-spec",
    "docs": "docker -i src -o apidocs -s -n -c vs",
    "compile": "coffee -c -o lib src",
    "clean": "rm -r apidocs lib ; true",
    "serve": "nodemon src/server.coffee -w src -w public -w data/infolis.tson -w ../mongoose-jsonld/src",
    "mongo": "mongod --dbpath data/mongodb --port 27018",
    "run": "npm run mongo & npm run serve",
    "prepublish": "npm run compile"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/infolis/infolis-web"
  },
  "author": "kba",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/infolis/infolis-web/issues"
  },
  "homepage": "https://github.com/infolis/infolis-web",
  "dependencies": {
    "async": "^1.5.0",
    "body-parser": "^1.12.2",
    "cors": "^2.7.1",
    "express": "^4.12.2",
    "express-jsonld": "0.0.15",
    "json-stringify-safe": "^5.0.1",
    "merge": "^1.2.0",
    "mongoose": "4.1.10",
    "mongoose-jsonld": "^0.1.35",
    "multiparty": "^4.1.1",
    "superagent": "^1.4.0",
    "swagger-ui": "^2.1.3",
    "tson": "0.0.6",
    "infolis-logging": "^0.0.1"
  },
  "devDependencies": {
    "tape": "^4.0.0",
    "docker": "^0.2.14",
    "tap-spec": "^2.2.2",
    "tapes": "^0.4.1"
  },
  "main": "lib/index.js"
}
