{
  "name": "infolis-web",
  "version": "0.0.4",
  "description": "Infolis Web services",
  "scripts": {
    "test": "coffee test/*.coffee | tap-spec",
    "docs": "docker -i src -o apidocs -s -n -c vs",
    "compile": "coffee -c -o lib src",
    "client": "browserify -t coffeeify -o public/client.js src-client/index.coffee",
    "watch-client": "nodemon -w src-client -e 'coffee' --exec 'npm run client'",
    "start": "npm run rebuild-deps; supervisor -w src/,data/infolis.tson,../jsonld-rapper/src,../express-jsonld/src,../mongoose-jsonld/src src/server.coffee",
    "rebuild-deps": "cd node_modules/express-jsonld/;npm run compile;cd -;",
    "mongo": "mongod --dbpath data/mongodb --port 27018",
    "run": "npm run mongo & npm run serve",
    "prepublish": "npm run compile"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/infolis/infolis-web"
  },
  "author": "kba",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/infolis/infolis-web/issues"
  },
  "homepage": "https://github.com/infolis/infolis-web",
  "dependencies": {
    "accepts": "^1.3.0",
    "async": "^1.5.0",
    "body-parser": "^1.12.2",
    "coffeeify": "^1.1.0",
    "cors": "^2.7.1",
    "express": "^4.12.2",
    "express-jsonld": "0.0.16",
    "infolis-logging": "^0.0.1",
    "jade": "^1.11.0",
    "json-stringify-safe": "^5.0.1",
    "jstransformer-coffee-script": "^1.0.0",
    "merge": "^1.2.0",
    "mongoose": "4.1.10",
    "mongoose-jsonld": "^0.1.36",
    "morgan": "^1.6.1",
    "multiparty": "^4.1.1",
    "superagent": "^1.4.0",
    "supervisor": "^0.9.1",
    "swagger-ui": "^2.1.3",
    "tson": "^0.0.6",
    "vim2html": ">=0.0.6"
  },
  "devDependencies": {
    "browserify": "^12.0.1",
    "coffee-script": "^1.10.0",
    "docker": "^0.2.14",
    "jquery": "^2.1.4",
    "lodash": "^3.10.1",
    "notie": "^2.1.0",
    "socket.io-client": "^1.3.7",
    "tap-spec": "^2.2.2",
    "tape": "^4.0.0",
    "tapes": "^0.4.1",
    "watchify": "^3.6.1"
  },
  "main": "lib/index.js"
}
